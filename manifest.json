{    
  "manifest_version": 3,    
  "name": "moonsh0p",    
  "version": "1.0",    
  "description": "Save a product from any page to multiple wishlists.",    
  "permissions": [    
    "activeTab",    
    "scripting",
    "storage"    
  ],    
  "host_permissions": [    
    "<all_urls>"    
  ],    
  "action": {    
    "default_popup": "popup.html",    
    "default_icon": {}    
  },    
  "icons": {}    
}    